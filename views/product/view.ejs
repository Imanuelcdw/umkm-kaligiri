<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>UMKM KALIGIRI</title>
    <%- include('../partials/css.ejs') %>
  </head>
  <body>
    <%- include('../dashboard/partials/header.ejs') %>
    <!--  -->
    <div class="container mt-3">
      <div class="card">
        <div class="card-body">
          <h3>Detail Produk</h3>

          <div class="list-group">
            <% data.forEach(e=>{ %>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">ID</h5>
              </div>
              <p class="mb-1"><%= e._id %></p>
            </a>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">Nama Produk</h5>
              </div>
              <p class="mb-1"><%= e.name %></p>
            </a>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">Gambar</h5>
              </div>
              <p class="mb-1"><%= e.image %></p>
            </a>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">Deskripsi</h5>
              </div>
              <p class="mb-1"><%= e.description %></p>
            </a>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">Ukuran</h5>
              </div>
              <p class="mb-1"><%= e.size %></p>
            </a>
            <a href="#" class="list-group-item list-group-item-action" aria-current="true">
              <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">Harga</h5>
              </div>
              <p class="mb-1"><%= e.price %></p>
            </a>
            <% }) %>
          </div>
        </div>
      </div>
    </div>
    <!--  -->
  </body>
</html>
<%- include('../partials/js.ejs') %>
